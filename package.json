{
  "name": "meetupbackend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "fix-code": "prettier-eslint --write 'source/**/*.{js,jsx}' ",
    "fix-styles": "prettier-stylelint --write 'source/**/*.{css,scss}' ",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "cookie-parser": "^1.4.6",
    "debug": "^2.6.9",
    "express": "^4.16.4",
    "express-bearer-token": "^2.4.0",
    "http-errors": "^1.6.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^6.2.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.2.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-prettier": "^3.3.1"
  }
}
